<% include header.ejs %>


<h2>Posts</h2>
<ul>
<% for(var i=0; i<posts.length; i++) {%>
  <li>
    <a href="<%= posts[i].relativeURL %>"><%= posts[i].title %></a>
  </li>
<% } %>
</ul>

<h2>Categories</h2>
<ul>
<% for(var i=0; i<categories.length; i++) {%>
  <li>
    <a href="categories/<%= categories[i] %>.html"><%= categories[i] %></a>
  </li>
<% } %>
</ul>

<% include footer.ejs %>